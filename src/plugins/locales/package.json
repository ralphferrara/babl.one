{
   "name": "@babl.one/locales",
   "version": "0.0.1",
   "description": "Wrapper to add language support to babl.one",
   "type": "module",
   "main": "dist/index.js",
   "types": "dist/index.d.ts",
   "exports": {
      ".": {
         "import": "./dist/index.js"
      }
   },
   "files": [
      "dist",
      "README.md"
   ],
   "scripts": {
      "dev": "tsx index.ts",
      "build": "tsup index.ts --target es2022 --format esm --dts --out-dir dist",
      "release": "node release.mjs",
      "postinstall": "node template.cjs"
   },
   "devDependencies": {
      "tsup": "^8.4.0",
      "tsx": "^4.19.3",
      "typescript": "^5.3.0"
   },
   "dependencies": {
      "@babl.one/core": "latest"
   },
   "peerDependencies": {}
}