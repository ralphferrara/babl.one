{
      "compilerOptions": {
        "target": "es2022",
        "module": "esnext",
        "moduleResolution": "bundler",
        "moduleDetection": "force",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "skipLibCheck": true,
        "declaration": true,
        "declarationMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
          "@babl.one/nextjs-core": ["../nextjs-core/src/index.ts"],
          "@babl.one/nextjs-core/*": ["../nextjs-core/src/*"]
        }
      },
      "include": [
        "index.ts"
      ],
      "exclude": [
        "node_modules",
        "dist"
      ]
    }
    