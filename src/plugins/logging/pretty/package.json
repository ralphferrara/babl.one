{
   "name": "@babl.one/plugin-logging-pretty",
   "version": "1.1.2",
   "description": "Pretty logger plugin for babl.one",
   "type": "module",
   "main": "dist/index.js",
   "types": "dist/index.d.ts",
   "exports": {
      ".": {
         "import": "./dist/index.js"
      }
   },
   "files": [
      "dist"
   ],
   "scripts": {
      "dev": "tsx index.ts",
      "build": "tsup index.ts --target es2022 --format esm --dts --out-dir dist",
      "release": "node release.mjs"
   },
   "devDependencies": {
      "tsup": "^8.4.0",
      "tsx": "^4.19.3",
      "typescript": "^5.3.0"
   }
}