      "release": "node release.mjs"
   },
   "devDependencies": {
      "@types/node": "^22.14.0",
      "@types/ws": "^8.18.1",
      "tsup": "^8.4.0",
      "tsx": "^4.19.3",
      "typescript": "^5.3.0"
   },
   "dependencies": {
      "cookie": "^1.0.2",
      "ws": "^8.18.1"
   },
   "peerDependencies": {
      "@babl.one/core": "latest"
   }
}