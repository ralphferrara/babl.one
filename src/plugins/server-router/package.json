{
   "name": "@babl.one/server-router",
   "version": "0.0.29",
   "description": "Handles Websocket and HTTP routing",
   "type": "module",
   "main": "dist/index.js",
   "types": "dist/index.d.ts",
   "exports": {
      ".": {
         "import": "./dist/index.js"
      }
   },
   "files": [
      "dist",
      "README.md"
