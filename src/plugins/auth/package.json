{
   "name": "@babl.one/auth",
   "version": "0.0.10",
   "description": "Handle the Authentication and Authorization of the Babl.one system",
   "type": "module",
   "main": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "exports": {
      ".": {
         "import": "./dist/index.js"
      }
   },
   "files": [
      "dist",
      "template",
      "template.cjs",
      "registerErrors.mjs",
      "init.cjs",
      "README.md"
   ],
   "scripts": {
      "dev": "cross-env NODE_ENV=development tsx src/index.ts",
      "build": "tsup index.ts --target es2022 --format esm --dts --out-dir dist",
      "release": "node release.mjs",
      "postinstall": "node template.cjs && node init.cjs"
   },
   "dependencies": {
      "@babl.one/jwt": "latest",
      "@babl.one/server-http": "latest",
      "@babl.one/server-router": "latest",
      "@babl.one/twofactor": "latest",
      "@babl.one/validate": "latest",
      "bcrypt": "latest"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/node": "^22.13.14",
      "@vitest/coverage-v8": "^3.0.9",
      "chokidar": "^4.0.3",
      "cross-env": "^7.0.3",
      "tsconfig-paths": "^4.2.0",
      "tsup": "^8.4.0",
      "tsx": "^4.19.3",
      "typescript": "^5.3.0",
      "vitest": "^3.0.9"
   },
   "peerDependencies": {
      "@babl.one/core": "@babl.one/core@latest"
   }
}