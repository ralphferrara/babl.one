{
   "name": "@babl.one/queue-rabbitmq",
   "version": "1.1.10",
   "description": "RabbitMQ queue plugin for babl.one",
   "type": "module",
   "main": "dist/index.js",
   "types": "dist/index.d.ts",
   "exports": {
      ".": {
         "import": "./dist/index.js"
      }
   },
   "files": [
