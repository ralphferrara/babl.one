      "release": "node release.mjs"
   },
   "devDependencies": {
      "@types/amqplib": "^0.10.7",
      "tsup": "^8.4.0",
      "tsx": "^4.19.3",
      "typescript": "^5.3.0"
   },
   "dependencies": {
      "amqplib": "^0.10.7"
   }
}