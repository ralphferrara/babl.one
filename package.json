{
   "name": "@babl.one/core",
   "version": "3.0.0",
   "description": "Bootstrap kernel for the babl.one framework",
   "type": "module",
   "main": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "exports": {
      ".": {
         "import": "./dist/index.js"
      }
   },
   "files": [
      "dist"
   ],
   "scripts": {
      "dev": "cross-env NODE_ENV=development tsx src/index.ts",
      "build": "cross-env NODE_ENV=production tsup src/index.ts --target es2022 --format esm --dts --out-dir dist",
      "release": "node release.mjs",
      "test": "vitest run --coverage"
   },
   "devDependencies": {
      "@types/amqplib": "^0.10.7",
      "@types/node": "^22.13.14",
      "@types/pg": "^8.11.11",
      "@vitest/coverage-v8": "^3.0.9",
      "cross-env": "^7.0.3",
      "jest": "^29.7.0",
      "tsup": "^8.4.0",
      "tsx": "^4.19.3",
      "typescript": "^5.3.0",
      "vitest": "^3.0.9"
   },
   "dependencies": {
      "amqplib": "^0.10.7",
      "cookie": "^1.0.2",
      "mongodb": "^6.15.0",
      "mysql2": "^3.14.0",
      "pg": "^8.14.1",
      "redis": "^4.7.0",
      "ws": "^8.18.1",
      "wss": "^0.0.2"
   }
}