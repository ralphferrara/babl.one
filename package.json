{
   "name": "@babl.one/core",
   "version": "3.1.52",
   "description": "Bootstrap kernel for the babl.one framework",
   "type": "module",
   "main": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "exports": {
      ".": {
         "import": "./dist/index.js"
      }
   },
   "bin": {
      "init": "./init.mjs"
   },
   "files": [
      "dist"
   ],
   "scripts": {
      "dev": "cross-env NODE_ENV=development tsx src/index.ts",
      "build": "npm run build:plugins && tsup src/index.ts --target es2022 --format esm --dts --out-dir dist",
      "build:plugins": "tsx build.plugins.ts",
      "release": "node release.mjs",
      "test": "vitest run --coverage"
   },
   "devDependencies": {
      "@types/node": "^22.13.14",
      "@vitest/coverage-v8": "^3.0.9",
      "chokidar": "^4.0.3",
      "cross-env": "^7.0.3",
      "tsconfig-paths": "^4.2.0",
      "tsup": "^8.4.0",
      "tsx": "^4.19.3",
      "typescript": "^5.3.0",
      "vitest": "^3.0.9"
   }
}