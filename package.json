{
   "name": "@babl.one/core",
   "version": "2.0.0",
   "description": "Bootstrap kernel for the babl.one framework",
   "type": "module",
   "main": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "exports": {
      ".": {
         "import": "./dist/index.js"
      }
   },
   "files": [
      "dist"
   ],
   "scripts": {
      "build": "tsup src/index.ts --target es2022 --format esm --dts --out-dir dist",
      "dev": "tsx src/index.ts",
      "release": "node release.mjs",
      "test": "vitest run --coverage"
   },
   "devDependencies": {
      "@types/node": "^22.13.14",
      "@vitest/coverage-v8": "^3.0.9",
      "tsup": "^7.2.0",
      "tsx": "^4.19.3",
      "typescript": "^5.3.0",
      "vitest": "^3.0.9"
   }
}